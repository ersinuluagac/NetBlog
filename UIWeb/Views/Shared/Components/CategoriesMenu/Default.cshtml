@model IEnumerable<Category>

<div class="nav-scroller py-1 mb-3 border-bottom">
  <nav style="white-space: nowrap;" class="nav nav-underline justify-content-between flex-nowrap overflow-auto">
    <a class="nav-item nav-link link-body-emphasis" asp-action="Index" asp-route-categoryId="">
        Tümü
      </a>
    @foreach (var category in Model)
    {
      <a class="nav-item nav-link link-body-emphasis" asp-action="Index" asp-route-categoryId="@category.Id">
        @category.Name
      </a>
    }
  </nav>
</div>
