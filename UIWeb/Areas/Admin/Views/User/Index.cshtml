@model IEnumerable<UserDto>

<div class="text-center py-3">
  <h1 class="display-1">Kullanıcılar</h1>
</div>
<table class="table">
  <thead class="">
    <tr>
      <td>Kullanıcı Adı</td>
      <td>E-posta Adresi</td>
      <td>Rol</td>
      <td></td>
    </tr>
  </thead>
  <tbody>
    @foreach (UserDto user in Model)
    {
        <tr>
          <td>@user.UserName</td>
          <td>@user.Email</td>
          <td>@user.Role</td>
          <td class="text-end">
            <div class="btn-group">
              <a href="##" class="btn btn-sm btn-outline-dark"><i class="fa-solid fa-magnifying-glass"></i></a>
              <a href="##" class="btn btn-sm btn-outline-danger"><i class="fa-solid fa-trash"></i></a>
            </div>
          </td>
        </tr>
    }
  </tbody>
</table>